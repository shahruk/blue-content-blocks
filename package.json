{
  "name": "content-blocks",
  "version": "0.0.1",
  "description": "Stencil Component Starter",
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.js",
  "es2015": "./dist/esm/index.mjs",
  "es2017": "./dist/esm/index.mjs",
  "types": "./dist/types/components.d.ts",
  "collection": "./dist/collection/collection-manifest.json",
  "collection:main": "./dist/collection/index.js",
  "unpkg": "./dist/content-blocks/content-blocks.esm.js",
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build --dev --docs",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "build-theme": "node-sass --output-style expanded --source-map true --source-map-contents true theme/stylesheet.scss src/theme-stylesheet.css",
    "lint": "eslint src/**/*{.ts,.tsx}",
    "lint-fix": "eslint src/**/*{.ts,.tsx} --fix"
  },
  "dependencies": {
    "@stencil/core": "^2.5.2",
    "rxjs": "^6.6.6",
    "swiper": "^6.5.0",
    "underscore": "^1.12.1",
    "video.js": "^7.11.4",
    "videojs-youtube": "^2.6.1"
  },
  "license": "MIT",
  "devDependencies": {
    "@stencil/eslint-plugin": "^0.3.1",
    "@stencil/sass": "^1.4.1",
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "@typescript-eslint/parser": "^4.16.1",
    "eslint": "^7.21.0",
    "eslint-plugin-react": "^7.22.0",
    "node-sass": "^5.0.0",
    "tslint": "^6.1.3",
    "typescript": "^4.2.3",
    "workbox-build": "^4.3.1"
  }
}
